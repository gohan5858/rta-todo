{
  "build": {
    "beforeDevCommand": "yarn dev",
    "beforeBuildCommand": "yarn build",
    "frontendDist": "../dist",
    "devUrl": "http://localhost:1420"
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  },
  "productName": "rta-todo",
  "mainBinaryName": "rta-todo",
  "version": "0.1.0",
  "identifier": "com.gohan.rta-todo",
  "plugins": {},
  "app": {
    "security": {
      "csp": null
    },
    "windows": [
      {
        "title": "rta-todo",
        "minWidth": 400,
        "minHeight": 400,
        "width": 500,
        "height": 700,
        "alwaysOnTop": true
      }
    ]
  }
}